  "scripts": {
    "dev": "next dev",
    "devssl": "local-ssl-proxy --key dev.key --cert devcert.pem --source 3000 --target 3001 | cross-env NODE_OPTIONS='--inspect' next dev -p 3001 ",
    "build": "next build",
    "start": "NODE_ENV=production ./node_modules/next/dist/bin/next start -p 8080",
    "analyze": "cross-env ANALYZE=true yarn build",
    "deploy-staging": "NODE_ENV=staging next build && NODE_ENV=staging next start -p 8080",
    "deploy-production": "cross-env NODE_ENV=production next build && cross-env NODE_ENV=production next start -p 8080",
    "format": "prettier src/**/*.js --write --ignore-unknown",
    "lint": "eslint --max-warnings 401 . || (echo \"\\033[0;31meslint error: too many warnings\\033[0m\" && exit 1)",
    "lint:changed": "eslint -c .eslintrc.js $(git diff --name-only --diff-filter=ACMRTUXB HEAD^1 | grep  -E \"(.js$|.ts$|.tsx$|.jsx$)\")",
    "script": "node",
    "test:e2e": "npm run cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "app:build": "sh -c 'npx webpack -c webpack.server.js --env appName=$1' --",
    "app:start": "sh -c 'node -r dotenv/config dist/$1/main.bundle.js' --",
    "app:start:watch": "./apps/watch.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1derwood/seoapp.git"
  },
  "browserslist": {
